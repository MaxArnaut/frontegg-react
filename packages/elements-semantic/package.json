{
  "name": "@frontegg/react-elements-semantic",
  "version": "1.0.8",
  "author": "FrontEgg LTD",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "export NODE_ENV='production'; rm -rf ./dist && rollup -c ../../scripts/rollup.config.js && echo DONE",
    "build:watch": "rollup -w -c ../../scripts/rollup.config.js",
    "test": "jest --runInBand --passWithNoTests -c ../../scripts/jest.config.json --rootDir . && echo DONE"
  },
  "dependencies": {
    "@frontegg/react-core": "^1.0.8",
    "classnames": "^2.2.6",
    "formik": "^1.5.7",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "semantic-ui-react": "^1.2.1",
    "underscore": "^1.10.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.8",
    "@types/underscore": "^1.10.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "dist/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "jsxBracketSameLine": true,
    "endOfLine": "lf",
    "tabWidth": 2
  },
  "standard": {
    "ignore": [
      "node_modules/",
      "dist/"
    ],
    "globals": [
      "describe",
      "it",
      "test",
      "expect",
      "afterAll",
      "jest"
    ]
  },
  "gitHead": "3fda4531d7941495d08a95e22b061afa3354cc05"
}
