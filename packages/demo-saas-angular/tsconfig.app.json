/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "allowSyntheticDefaultImports": true,
    "paths": {

      "@helpers": [
        "../../node_modules/@frontegg/react/dist/cjs/helpers"
      ],
      "@helpers/*": [
        "../../node_modules/@frontegg/react/dist/cjs/helpers/*"
      ],
      "@components": [
        "../../node_modules/@frontegg/react/dist/cjs/components"
      ],
      "@components/*": [
        "../../node_modules/@frontegg/react/dist/cjs/components/*"
      ],
      "@api": [
        "../../node_modules/@frontegg/react/dist/cjs/api"
      ],
      "@api/*": [
        "../../node_modules/@frontegg/react/dist/cjs/api/*"
      ],
      "@elements": [
        "../../node_modules/@frontegg/react/dist/cjs/elements"
      ],
      "@elements/*": [
        "../../node_modules/@frontegg/react/dist/cjs/elements/*"
      ],
      "@providers": [
        "../../node_modules/@frontegg/react/dist/cjs/providers"
      ],
      "@providers/*": [
        "../../node_modules/@frontegg/react/dist/cjs/providers/*"
      ],
      "@plugins": [
        "../../node_modules/@frontegg/react/dist/cjs/plugins"
      ],
      "@plugins/*": [
        "../../node_modules/@frontegg/react/dist/cjs/plugins/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
