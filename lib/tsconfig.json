{
  "compilerOptions": {
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "es2019",
      "es2020",
      "esnext",
      "dom"
    ],
    "target": "es6",
    "noEmitHelpers": true,
    "importHelpers": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "strictPropertyInitialization": true,
    "module": "esnext",
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmit": true
  },

}
